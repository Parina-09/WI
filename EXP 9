import numpy as np
from sklearn.metrics.pairwise import euclidean_distances

A = np.array([1,1,1,0,0]) 
B = np.array([0, 1, 1, 1, 1]) 
euclidean_dist= np.linalg.norm(A-B)
similarity_manual=1/(1+euclidean_dist)

print(f"Euclidean Distance(manual): {euclidean_dist:.2f}")
print(f"Similarity(manual): {similarity_manual}")


euclidean_dist_sklearn = euclidean_distances([A],[B])[0][0]
similarity_sklearn = 1/(1+euclidean_dist_sklearn)

print("Euclidean Distance(sklearn):", euclidean_dist_sklearn)
print("Similarity(sklearn):", similarity_sklearn)
