import numpy as np
from sklearn.metrics.pairwise import cosine_similarity

A=np.array([1, 1, 1, 0, 0]) 
B= np.array([0, 1, 1, 1, 1]) 

dot_product = np.dot(A, B)
norm_A=np.linalg.norm(A)
norm_B= np.linalg.norm(B)
cosine_sim_manual = dot_product/(norm_A* norm_B)

print(f"Cosine Similarity (manual): {cosine_sim_manual:.2f}")

 


cosine_sim_sklearn = cosine_similarity([A], [B])[0][0]
print(f"Cosine Similarity (sklearn): {cosine_sim_sklearn:.2f}")
